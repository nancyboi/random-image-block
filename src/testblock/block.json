{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/testblock",
	"version": "0.1.0",
	"title": "Random Image Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "A block to display a random image",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "testblock",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
    "images": {
        "type": "array",
        "default": []
    },
    "enableParallax": {
        "type": "boolean",
        "default": false
    }
}
}
